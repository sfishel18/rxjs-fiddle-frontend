    "babel-loader": "^8.1.0",
    "css-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.5.4",
    "loki": "^0.25.1",
    "prettier": "^1.18.2",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^4.0.5",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
